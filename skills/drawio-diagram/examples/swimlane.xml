<?xml version="1.0" encoding="UTF-8"?>
<!-- 
  Example: Swimlane Diagram
  Demonstrates swimlanes with steps and cross-lane connections
  IMPORTANT: All mxCell elements are siblings - edges are NOT nested
-->
<root>
<mxCell id="0"/>
<mxCell id="1" parent="0"/>

<mxCell id="lane_frontend" value="Frontend" style="swimlane;startSize=30;fillColor=#e1d5e7;strokeColor=#9673a6;" vertex="1" parent="1">
  <mxGeometry x="40" y="40" width="200" height="300" as="geometry"/>
</mxCell>

<mxCell id="lane_backend" value="Backend" style="swimlane;startSize=30;fillColor=#dae8fc;strokeColor=#6c8ebf;" vertex="1" parent="1">
  <mxGeometry x="280" y="40" width="200" height="300" as="geometry"/>
</mxCell>

<mxCell id="lane_db" value="Database" style="swimlane;startSize=30;fillColor=#d5e8d4;strokeColor=#82b366;" vertex="1" parent="1">
  <mxGeometry x="520" y="40" width="200" height="300" as="geometry"/>
</mxCell>

<mxCell id="fe_step1" value="User Input" style="rounded=1;whiteSpace=wrap;html=1;" vertex="1" parent="lane_frontend">
  <mxGeometry x="20" y="60" width="160" height="40" as="geometry"/>
</mxCell>

<mxCell id="fe_step2" value="Validate Form" style="rounded=1;whiteSpace=wrap;html=1;" vertex="1" parent="lane_frontend">
  <mxGeometry x="20" y="130" width="160" height="40" as="geometry"/>
</mxCell>

<mxCell id="fe_step3" value="Display Result" style="rounded=1;whiteSpace=wrap;html=1;" vertex="1" parent="lane_frontend">
  <mxGeometry x="20" y="230" width="160" height="40" as="geometry"/>
</mxCell>

<mxCell id="be_step1" value="API Request" style="rounded=1;whiteSpace=wrap;html=1;" vertex="1" parent="lane_backend">
  <mxGeometry x="20" y="130" width="160" height="40" as="geometry"/>
</mxCell>

<mxCell id="be_step2" value="Process Data" style="rounded=1;whiteSpace=wrap;html=1;" vertex="1" parent="lane_backend">
  <mxGeometry x="20" y="200" width="160" height="40" as="geometry"/>
</mxCell>

<mxCell id="db_step1" value="Query Data" style="rounded=1;whiteSpace=wrap;html=1;" vertex="1" parent="lane_db">
  <mxGeometry x="20" y="200" width="160" height="40" as="geometry"/>
</mxCell>

<mxCell id="edge1" style="edgeStyle=orthogonalEdgeStyle;endArrow=classic;" edge="1" parent="1" source="fe_step1" target="fe_step2">
  <mxGeometry relative="1" as="geometry"/>
</mxCell>

<mxCell id="edge2" style="edgeStyle=orthogonalEdgeStyle;endArrow=classic;exitX=1;exitY=0.5;entryX=0;entryY=0.5;" edge="1" parent="1" source="fe_step2" target="be_step1">
  <mxGeometry relative="1" as="geometry"/>
</mxCell>

<mxCell id="edge3" style="edgeStyle=orthogonalEdgeStyle;endArrow=classic;" edge="1" parent="1" source="be_step1" target="be_step2">
  <mxGeometry relative="1" as="geometry"/>
</mxCell>

<mxCell id="edge4" style="edgeStyle=orthogonalEdgeStyle;endArrow=classic;exitX=1;exitY=0.5;entryX=0;entryY=0.5;" edge="1" parent="1" source="be_step2" target="db_step1">
  <mxGeometry relative="1" as="geometry"/>
</mxCell>

<mxCell id="edge5" value="Response" style="edgeStyle=orthogonalEdgeStyle;endArrow=classic;dashed=1;exitX=0;exitY=0.5;entryX=1;entryY=0.5;" edge="1" parent="1" source="db_step1" target="fe_step3">
  <mxGeometry relative="1" as="geometry">
    <Array as="points">
      <mxPoint x="260" y="280"/>
      <mxPoint x="260" y="290"/>
    </Array>
  </mxGeometry>
</mxCell>

</root>
